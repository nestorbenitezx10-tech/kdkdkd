class Person:
    def __init__(self, name, dob, gender, status, houseNo, purok, brgy, cityMun, province, country):
    # def __init__(self, name, dob, gender, status, address):
        self.__name = name #Name object
        self.__dob = dob #Date object 10/19/2020
        self.__gender = gender
        self.__status = status
        # self.__address = address
        self.__address = Address(houseNo, purok, brgy, cityMun, province, country)

    def __str__(self):
        return "\nName: " + self.__name + "\nDate of Birth: " + self.__dob + \
            "\nGender: " + self.__gender + "\nStatus: " + self.__status + \
                "\nAddress: " + str(self.__address)

    # incomplete class defn for Person


class Employee(object):
    def __init__(self, name, dob, gender, status, houseNo, purok, brgy, cityMun, province, country, occ, pos, sal):
        self.__name = name
        self.__dob = dob
        self.__gender = gender
        self.__status = status
        self.__address = Address(houseNo, purok, brgy, cityMun, province, country)
        self.__occupation = occ
        self.__position = pos
        self.__salary = sal

    # incomplete employee class def'n
        
    def __str__(self):
        return "\nName: " + self.__name + "\nDate of Birth: " + self.__dob + \
            "\nGender: " + self.__gender + "\nStatus: " + self.__status + \
                "\nAddress: " + str(self.__address) + "\nOccupation: " + self.__occupation + \
                    "\nPosition: " + self.__position + "\nSalary: " + str(self.__salary)

class Student:
    # same data fields with Person, add specific for student
    def __init__(self, name, dob, gender, status, houseNo, purok, brgy, cityMun, province, country, studID, course, year):
        self.__name = name
        self.__dob = dob
        self.__gender = gender
        self.__status = status
        self.__address = Address(houseNo, purok, brgy, cityMun, province, country)
        self.__studID = studID
        self.__course = course
        self.__year = year
    
    def __str__(self):
        return "\nName: " + self.__name + "\nDate of Birth: " + self.__dob + \
            "\nGender: " + self.__gender + "\nStatus: " + self.__status + \
                "\nAddress: " + str(self.__address) + "\nStudent ID: " + str(self.__studID) + \
                "\nCourse: " + str(self.__course) + "\nYear Level: " + str(self.__year)

class DOB:
    def __init__(self, m, d, y):
        self.__month = m
        self.__day = d
        self.__year = y

class Name:
    pass
    # last, first, middle, suffix

class Address:
    def __init__(self, houseNo, purok, brgy, cityMun, province, country):
        self.__houseNo = houseNo
        self.__purok = purok
        self.__brgy = brgy
        self.__cityMun = cityMun
        self.__province = province
        self.__country = country
    
    # incomplete address class def'n
    def __str__(self):
        return "Purok " + str(self.__purok) + ", " + str(self.__houseNo) + \
            ", " + self.__brgy + ", " + self.__cityMun + \
                ", " + self.__province + ", " + self.__country

